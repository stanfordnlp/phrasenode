{
  "manifest_version": 2,
  "name": "phrasenode demo",
  "description": "",
  "version": "1",
  "background": {
    "scripts": [ "jquery-3.4.1.min.js", "background.js" ]
  },
  "permissions": [
    "activeTab",
    "http://127.0.0.1/"
  ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "jquery-3.4.1.min.js", "content.js" ],
      "css": [ "content.css" ]
    }
  ],
  "browser_action": {
    "default_title": "phrasenode demo"
  }
}
